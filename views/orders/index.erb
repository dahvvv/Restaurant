<h1>Orders for Table Number <%=@table_number%></h1>
<table>
  <th>Food</th>
  <th>Order Id</th>
  <th>Price</th>
  <% @orders.each do |order| %>
    <tr>
      <td>
        <a href="/orders/<%=order.id%>"><%=order.food_name%></a>
      </td>
      <td>
        <%=order.id%>
      </td>
      <td>
        $<%=order.charge%>
      </td>
    </tr>
  <% end %>
</table>

<a href="/parties/<%=@party_id%>">Back To Party</a>
<br>
<a href="/">Back To Home</a>