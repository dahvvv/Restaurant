<h1>Foods</h1>
<table>
  <th>Food Name</th>
  <th>Cuisine</th>
  <th>Price</th>
  <th>Allergens?</th>
  <% @foods.each do |food| %>
    <tr>
      <td><a class="foods_index" href="/foods/<%=food.id%>"><%= food.name %></a></td>
      <td><%= food.cuisine_type %></td>
      <td>$<%= food.priceprint %></td>
      <td><%= food.allergens %></td>
    </tr>
  <% end %>
</table>
<h2>Add a <a href="/foods/new">new</a> food?</h2>

<footer class="footer">
  <span id="go_home"><a href="/">HOME VIEW</a></span>
</footer>