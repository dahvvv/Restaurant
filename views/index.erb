<nav class="navbar">
  <span id="add_party"><a href="/parties/new">Add A Party</a></span>
  <span id="view_parties"><a href="/parties">See All Parties</a></span>
</nav>

<div class="tables">
  <% @parties.each do |party| %>
    <div class="indiv_table <%=party.paid%>">
      <a href="/parties/<%=party.id%>"><h3 class="table_num cf"><%= party.table_number %></h3></a>
      <h6 class="guests_num"><%= party.number_of_guests %> GUESTS</h6>
    </div>
  <% end %>
</div>


<a href="/foods"><h1>Foods</h1></a>
<table>
  <th>Food Name</th>
  <th>Cuisine</th>
  <th>Price</th>
  <th>Allergens?</th>
  <th>Food ID</th>
  <% @foods.each do |food| %>
    <tr>
      <td><a href="/foods/<%=food.id%>"><%= food.name %></a></td>
      <td><%= food.cuisine_type %></td>
      <td>$<%= food.priceprint %></td>
      <td><%= food.allergens %></td>
      <td><%= food.id %></td>
    </tr>
  <% end %>
</table>
<h2>Add a <a href="/foods/new">new</a> food?</h2>
