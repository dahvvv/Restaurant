<h1>Edit Party</h1>
<form action="/parties/<%=@party.id%>" method="post">
  <input type="hidden" name="_method" value="patch" />
  <label for="party[name]">Table Number</label>
  <input type="text" name="party[table_number]" value="<%=@party.table_number%>" />
  <br>
  <label for="party[number_of_guests]">Number Of Guests</label>
  <input type="text" name="party[number_of_guests]" value="<%=@party.number_of_guests%>" />
  <br>
  <label for="party[paid]">Paid?</label>
  <input type="text" name="party[paid]" value="<%=@party.paid%>" />
  <br>
  <input type="submit">
</form>
